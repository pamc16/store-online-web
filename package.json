{
	"name": "store-online-web",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@ant-design/icons": "^5.4.0",
		"@emotion/css": "^11.13.0",
		"@emotion/react": "^11.13.0",
		"@reduxjs/toolkit": "^1.9.7",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.5.2",
		"@types/node": "20.14.11",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/react-google-recaptcha": "^2.1.9",
		"@types/react-router-dom": "^5.3.3",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"antd": "^5.19.3",
		"axios": "^1.7.2",
		"firebase": "^10.12.4",
		"react": "18.3.1",
		"react-dom": "^18.3.1",
		"react-firebase-hooks": "^5.1.1",
		"react-google-recaptcha": "^3.1.0",
		"react-intersection-observer": "^9.13.0",
		"react-redux": "^8.1.3",
		"react-router-dom": "^6.25.1",
		"react-scripts": "5.0.1",
		"react-spinners": "^0.14.1",
		"redux": "^4.2.1",
		"redux-thunk": "^2.4.2",
		"vite": "^5.3.4",
		"vite-plugin-checker": "^0.7.2",
		"vite-plugin-html": "^3.2.2",
		"vite-plugin-svgr": "^4.2.0",
		"vite-plugin-webfont-dl": "^3.9.4",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^2.0.4",
		"web-vitals": "^2.1.4",
		"wireit": "^0.14.5"
	},
	"scripts": {
		"start": "vite",
		"build": "wireit",
		"lint": "wireit",
		"format": "wireit",
		"stylelint": "wireit",
		"preview": "wireit"
	},
	"wireit": {
		"build": {
			"command": "vite build",
			"clean": "if-file-deleted",
			"output": [
				"dist/**"
			],
			"files": [
				"src/**",
				"public/**",
				"index.html",
				"tsconfig.json",
				"vite.config.ts",
				"!**/*.{spec,test}.*",
				"!**/__{tests,mocks,fixtures}__/*"
			]
		},
		"preview": {
			"command": "vite preview",
			"dependencies": [
				"build"
			],
			"env": {
				"BASE_URL": "/"
			}
		},
		"format": {
			"command": "prettier --cache --write **/*.{ts,cts,mts,tsx,css,scss,sass,json,html,md,yml,yaml,webmanifest}",
			"output": [
				"./node_modules/.cache/prettier/.prettier-cache"
			],
			"files": [
				".prettierrc",
				".prettierignore",
				"**/*.{ts,cts,mts,tsx,css,scss,sass,json,html,md,yml,yaml,webmanifest}"
			]
		},
		"lint": {
			"command": "eslint --cache --fix src/**/*.{ts,cts,mts,tsx}",
			"output": [
				".eslintcache"
			],
			"files": [
				"eslint.config.js",
				".prettierrc",
				".prettierignore",
				"**/*.{js,mjs,cjs,jsx,ts,cts,mts,tsx,json}"
			],
			"dependencies": [
				"stylelint"
			]
		},
		"stylelint": {
			"command": "stylelint --cache --fix src/**/*.{css,scss,sass}",
			"output": [
				".stylelintcache"
			],
			"files": [
				".stylelintrc",
				".stylelintignore",
				"src/**/*.{css,scss,sass}"
			]
		}
	},
	"type": "module",
	"engines": {
		"node": ">=20",
		"npm": ">=9",
		"pnpm": ">=8"
	},
	"devDependencies": {
		"typescript": "5.5.4"
	}
}
